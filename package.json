{
  "name": "figma-icons-tokens",
  "version": "1.2.0",
  "description": "Figma-icons-tokens aims to import figma icons given a design tokens configuration file exposed by Tokens Studio Figma.",
  "main": "./lib/index.js",
  "bin": {
    "figma-icons-tokens": "./bin/index.js"
  },
  "scripts": {
    "build": "node bin/index.js --file=tokens.json --theme=global --key=icon --path=assets"
  },
  "keywords": [
    "cli",
    "Figma",
    "design",
    "tokens",
    "Design Tokens",
    "Design System",
    "Tokens Studio Figma"
  ],
  "author": "Javier Samper Arias",
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "utils",
    "LICENSE",
    "REAMDE.md"
  ],
  "dependencies": {
    "colors": "1.4.0",
    "dotenv": "16.0.3",
    "minimist": "1.2.7",
    "node-fetch": "3.3.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jsamper92/figma-icons-tokens.git"
  },
  "bugs": {
    "url": "https://github.com/Jsamper92/figma-icons-tokens/issues"
  },
  "homepage": "https://github.com/Jsamper92/figma-icons-tokens#readme"
}
